<?php

namespace App\Containers\{{container-name}}\Actions;

use App\Ship\Parents\Actions\Action;
use App\Ship\Parents\Transporters\Transporter;
use HiveApi\Core\Foundation\Facades\Hive;

class Delete{{model}}Action extends Action
{
    public function run(Transporter $transporter)
    {
        ${{entity}} = Hive::call('{{container-name}}@Find{{model}}ByIdTask', [$transporter->id]);

        return Hive::call('{{container-name}}@Delete{{model}}Task', [${{entity}}]);
    }
}
